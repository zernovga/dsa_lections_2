{
  "name": "lections",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "slidev build",
    "dev": "slidev --open",
    "export": "slidev export"
  },
  "dependencies": {
    "@slidev/cli": "^0.49.24",
    "@slidev/theme-default": "latest",
    "@slidev/theme-seriph": "latest",
    "plotly.js": "^2.35.0",
    "slidev-addon-graph": "^0.0.4",
    "slidev-addon-tldraw": "^0.3.0",
    "slidev-theme-sirius-college": "^0.4.1",
    "uuid": "^10.0.0",
    "vue": "^3.4.33",
    "vue3-plotly": "^0.0.7"
  },
  "devDependencies": {
    "playwright-chromium": "^1.20.0",
    "prettier": "^3.3.3",
    "prettier-plugin-slidev": "^1.0.5"
  },
  "prettier": {
    "overrides": [
      {
        "files": [
          "slides.md",
          "pages/*.md",
          "pages/*/*.md",
          "slides/*/*.md",
          "slides/*.md",
          "slides/*/code/*.py"
        ],
        "options": {
          "parser": "slidev",
          "plugins": [
            "prettier-plugin-slidev"
          ]
        }
      }
    ]
  }
}